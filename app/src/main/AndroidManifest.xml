            </intent-filter>
        </service>
        <service
            android:name=".TamaYankeesFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>

